<Window x:Class="ProyectoFinalServicioCliente.UI.Login.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoFinalServicioCliente.UI.Login"
         xmlns:PasswordHelper="clr-namespace:ProyectoFinalServicioCliente"
        mc:Ignorable="d"
        Title="Login" Height="210" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="10">
            <StackPanel Orientation="Vertical">
                <Label Content="Nombre Usuario"/>

                <TextBox x:Name="NombreUsuarioTextBox" Text="{Binding NombreUsuario}" />

                <Label Content="Contraseña"/>
                    
                

                <Button x:Name="IngresarButton" Content="Ingresar" Margin="30" Width="60" Height="24" Click="IngresarButton_Click1"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
