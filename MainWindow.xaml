        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoFinalServicioCliente"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Menu Height="20" VerticalAlignment="Top">
            <MenuItem Header="Login" Click="MenuItem_Click"/><!--Para ver como quedo el WPF del login se creo este objeto-->
            <MenuItem Header="Registro Usuario" Click="MenuItem_Click_1"/>
            <MenuItem Header="Registro Articulo" Click="MenuItem_Click_2"/>
            <MenuItem Header="Registro Categoria" Click="MenuItem_Click_3"/>
            <MenuItem Header="Registro Compras" Click="MenuItem_Click_4"/>
            <!--Para ver como quedo el WPF del Registro Usuario se creo este objeto-->
        </Menu>
    </Grid>
</Window>
